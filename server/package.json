{
  "name": "devconnector",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "client-install": "npm install --prefix ../client",
    "start": "babel-node server.js",
    "server": "nodemon --exec babel-node server.js",
    "client": "npm start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest",
    "now-dev-server": "now dev -p 4000",
    "now-dev-client": "now dev -p 3000 ../client/",
    "now-dev": "npm run now-dev-server & npm run now-dev-client",
    "now-stop": "pkill now"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/node_modules/regenerator-runtime/runtime"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx$": "babel-jest"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "gravatar": "^1.8.0",
    "jest": "^24.8.0",
    "jsonwebtoken": "^8.4.0",
    "jss-extend": "^6.2.0",
    "mongo-uri-builder": "^2.0.0",
    "mongoose": "^5.6.0",
    "nodemailer": "^6.2.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "regenerator-runtime": "^0.13.1",
    "sequelize": "^5.8.9",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "node-mocks-http": "^1.7.6",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
  }
}
